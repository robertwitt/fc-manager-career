{
  "name": "fc-manager-career",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "^1.124.3",
    "axios": "^1.7.7",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "start": "cds-serve",
    "test": "jest",
    "watch-manage-club-master": "cds watch --open manage-club-master/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-start-manager-career": "cds watch --open start-manager-career/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy fc-manager-career --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "sapux": [
    "app/manage-club-master",
    "app/start-manager-career"
  ],
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[development]": {
        "auth": {
          "kind": "mocked",
          "users": {
            "admin": {
              "password": "",
              "roles": [
                "Administrator"
              ]
            }
          }
        }
      },
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      }
    }
  }
}
